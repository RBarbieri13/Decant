{
  "name": "decant",
  "productName": "Decant",
  "version": "0.1.0",
  "description": "AI-Powered Knowledge Curation Platform with dual hierarchy organization",
  "type": "module",
  "main": "dist/server.js",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:client": "vite",
    "dev:server": "tsx watch src/server.ts",
    "build": "vite build && tsc -p tsconfig.server.json",
    "start": "node dist/server.js",
    "preview": "vite preview",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:all": "vitest run && playwright test",
    "migrate": "tsx src/backend/database/migrations/cli.ts",
    "migrate:status": "tsx src/backend/database/migrations/cli.ts status",
    "migrate:up": "tsx src/backend/database/migrations/cli.ts up",
    "migrate:create": "tsx src/backend/database/migrations/cli.ts create"
  },
  "keywords": [
    "knowledge-base",
    "ai",
    "organization"
  ],
  "author": {
    "name": "RBarbieri13",
    "email": "rbarbieri13@gmail.com"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.13",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.6",
    "@types/pino-http": "5.8.4",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^10.0.0",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^2.1.8",
    "concurrently": "8.2.2",
    "supertest": "^7.0.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vite": "^5.4.21",
    "vitest": "^2.1.8"
  },
  "dependencies": {
    "@types/cheerio": "^0.22.35",
    "better-sqlite3": "^11.7.0",
    "cheerio": "^1.1.2",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-rate-limit": "^7.5.0",
    "openai": "^6.16.0",
    "pino": "10.3.0",
    "pino-http": "11.0.0",
    "pino-pretty": "13.1.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "uuid": "^9.0.1",
    "zod": "4.3.6"
  }
}
